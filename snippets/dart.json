{
    "fxml_app": {
        "prefix": "fxml_app",
        "body": [
          "import 'package:flutter/material.dart';",
          "import 'package:flutter_xmllayout_helpers/providers/PipeProvider.dart';",
          "import 'package:provider/provider.dart';",
          "import 'package:flutter_localizations/flutter_localizations.dart';",
          "import 'pipes/translate.dart';",
          "import 'i18n/gen/delegate.dart';",
          "",
          "",
          "void main() async {",
          "  final RouteObserver<PageRoute> routeObserver = new RouteObserver<PageRoute>();",
          "",
          "  runApp(",
          "    MultiProvider(",
          "      //",
          "      // dependency injection",
          "      //",
          "      providers: [",
          "        Provider<RouteObserver<Route>>.value(value: routeObserver),",
          "        Provider<PipeProvider>(builder: _createPipeProvider)",
          "      ],",
          "      child: ${1:My}App(routeObserver: routeObserver)",
          "    )",
          "  );",
          "}",
          "",
          "PipeProvider _createPipeProvider(BuildContext context) {",
          "  final PipeProvider pipeProvider = PipeProvider();",
          "",
          "  //",
          "  // register pipes",
          "  //",
          "  pipeProvider.register(new TranslatePipe());",
          "",
          "  return pipeProvider;",
          "}",
          "",
          "class ${1:My}App extends StatelessWidget {",
          "  const ${1:My}App({",
          "    Key key,",
          "    this.routeObserver",
          "  }) : super(key: key);",
          "",
          "  final RouteObserver<PageRoute> routeObserver;",
          "",
          "  @override",
          "  Widget build(BuildContext context) {",
          "",
          "    return MaterialApp(",
          "      debugShowCheckedModeBanner: false,",
          "      navigatorObservers: [routeObserver],",
          "      title: '${2:MyAppTitle}',",
          "",
          "      //",
          "      // theming",
          "      //",
          "      theme: ThemeData(primarySwatch: Colors.blue),",
          "      ",
          "      //",
          "      // localization",
          "      //",
          "      localizationsDelegates: [",
          "        GlobalMaterialLocalizations.delegate,",
          "        GlobalWidgetsLocalizations.delegate,",
          "        AppLocalizationsDelegate()",
          "      ],",
          "      supportedLocales: [",
          "        Locale(\"en\")",
          "      ],",
          "      locale: Locale(\"en\"),",
          "      ",
          "      //",
          "      // navigation",
          "      //",
          "      home: HomePage(),",
          "      onGenerateRoute: (RouteSettings settings) {",
          "        switch (settings.name) {",
          "          case '/':",
          "            return MaterialPageRoute(builder: (context) => HomePage());",
          "        }",
          "      }",
          "    );",
          "  }",
          "}",
          ""
        ]
    }
}